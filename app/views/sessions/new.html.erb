<div class="container">
<br>
<%= image_tag('store.jpg', style: "height: 600px; width: 100%") %>
<h1 align="center">Falmouth Falcons-Buy and Sell Store</h1>

<div class="centered">

  <h1 class="white">Log In</h1>

  <%= form_tag sessions_path do %>
     <div class="field">
     <h6 class="white"> <%= label_tag :email %><br/></h6>
      <%= text_field_tag :email, params[:email] %>
    </div>
    <div class="field">
      <h6 class="white"><%= label_tag :password %><br /></h6>
      <%= password_field_tag :password %>
    </div>
    <hr>
    <div class="actions"><%= submit_tag "Log In", class: "btn, btn-primary" %></div>
  <% end %>
  </div>
</div>